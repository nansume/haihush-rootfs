tries=3
waitretry=5
wait=6
header=Accept-Encoding:
restrict_file_names=nocontrol,unix
follow_ftp=off
passive_ftp=off
glob=off
check_certificate=off
hsts=off
#robots=off
#http_keep_alive=off
cookies=off
background=off
max_redirect=0
limit_rate=90k
prefer_family=IPv4
random_wait=on
recursive=off
no_parent=on
relative_only=off
referer="https://www.google.com/search?q=news"
user_agent="Mozilla/5.0 (Windows; U; Windows NT 6.1; en-US) AppleWebKit/534.10 (KHTML, like Gecko) Chrome/7.0.540.0 Safari/534.10"
use_proxy=on
http_proxy=http://10.0.0.1:80/
https_proxy=http://10.0.0.1:80/
ftp_proxy=http://10.0.0.1:80/
#no_proxy=localhost,127.0.0.1
verbose=on
logfile=~/.log/wgetrc.log
